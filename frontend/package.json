{
  "name": "recall-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "generate-api": "openapi-ts -f openapi-ts.config.ts",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prettier": "prettier --config .prettierrc.json -w ./src",
    "lint": "eslint --fix ./src",
    "ts-check": "tsc",
    "checks": "npm run prettier && npm run lint && npm run ts-check"
  },
  "dependencies": {
    "@gotfeedback/markdown-it-media": "^1.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@mdit/plugin-tex": "^0.16.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "immer": "^11.1.4",
    "jotai": "^2.18.0",
    "katex": "^0.16.32",
    "lucide-react": "^0.476.0",
    "markdown-it": "^14.1.1",
    "micromatch": "^4.0.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.2",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.2",
    "ts-pattern": "^5.9.0",
    "wouter": "^3.9.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@hey-api/client-fetch": "^0.3.4",
    "@hey-api/openapi-ts": "^0.53.12",
    "@tailwindcss/vite": "^4.2.0",
    "@types/katex": "^0.16.8",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/typescript-estree": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vite-plugin-webfont-dl": "^3.12.0",
    "vitest": "^4.0.18"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
